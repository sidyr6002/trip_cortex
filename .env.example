# Local development environment variables
# Copy to .env and fill in values: cp .env.example .env

# PostgreSQL (local Docker)
AURORA_HOST=localhost
AURORA_PORT=5432
AURORA_DB=tripcortex
AURORA_USER=tripcortex
AURORA_PASSWORD=localdev

# DynamoDB Local
DYNAMODB_ENDPOINT=http://localhost:8000

# AWS (for Bedrock calls in dev account)
AWS_REGION=us-east-1
AWS_PROFILE=trip-cortex-dev

# Clerk (local dev only â€” use Secrets Manager in deployed envs)
CLERK_SECRET_KEY=sk_test_xxxxx
CLERK_PUBLISHABLE_KEY=pk_test_xxxxx

# Bedrock model IDs
NOVA_2_LITE_MODEL_ID=us.amazon.nova-2-lite-v1:0
NOVA_MME_MODEL_ID=amazon.nova-2-multimodal-embeddings-v1:0

# Environment
ENVIRONMENT=local
LOG_LEVEL=DEBUG
